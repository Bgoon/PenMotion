<UserControl x:Class="PendulumMotionEditor.Resource.Image.Button_AddMotion"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PendulumMotionEditor.Resource.Image"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Canvas>
        <Canvas.Resources>
            <SolidColorBrush Color="#FFFFFF" x:Key="FillColor"></SolidColorBrush>
        </Canvas.Resources>
        <Polygon Fill="{StaticResource FillColor}" Points="18.6,7.6 11,7.6 11,0 7.6,0 7.6,7.6 0,7.6 0,11 7.6,11 7.6,18.6 11,18.6 11,11 18.6,11 "></Polygon>
    </Canvas>
</UserControl>
