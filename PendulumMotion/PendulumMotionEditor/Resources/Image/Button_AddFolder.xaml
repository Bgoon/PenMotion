<UserControl x:Class="PendulumMotionEditor.Resources.Image.Button_AddFolder"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PendulumMotionEditor.Resources.Image"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Canvas>
        <Canvas.Resources>
            <SolidColorBrush Color="#FFFFFF" x:Key="FillColor"></SolidColorBrush>
        </Canvas.Resources>
        <Polygon Fill="{StaticResource FillColor}" Points="11.7,0 10.1,1.7 0,1.7 0,16.5 18.6,16.5 18.6,1.7 18.6,0 "></Polygon>
    </Canvas>
</UserControl>
